//Elever i elevhem
String dormitory = cbx_dormitory.getSelectedItem().toString();
        String st = "";
        String yes = "Elever som tillhör";
        if(dormitory.isEmpty()){
            st ="";
            jTextArea1.setText("Välj elevhem");
        }
        else
        
        st = Getter.getEleverFromElevhem(hwdb, dormitory);
        jTextArea1.setText(yes + " " + dormitory + "\n" +st);
        
        
//Lärare, kurser, datum
String startDate = tf_startdatum.getText();
        String stopDate = tf_slutDatum.getText();
        String firstname = jTextField1.getText();
        String surname = jTextField2.getText();
        String st ="";
        try{ 
            
            st = Getter.getCourseAndTeacher(hwdb, startDate, stopDate, firstname, surname);
            jTextArea1.setText(firstname + " " + surname + " har undervisat följande kurser mellan " + startDate + " och " + stopDate + "\n" +st + "\n");
        }
        catch(Exception e){
            st = "error, kika med supporten";
            jTextArea1.setText(st);
        }
        
        
        
//Elevhemspokalen ställning
String st = "";
        try{
            st = Getter.getElevhemspokalen(hwdb);
            jTextArea1.setText(st + "\n");
        }
        catch(Exception e){
            
        }
        
        
        
        
        
//Elev registrerad på kurser
String st ="";
        
        try{
            st = Getter.getCourseAndStudent(hwdb, jTextField1.getText(), jTextField2.getText());
            jTextArea1.setText(st);
        }
        catch(Exception e){
            
        }
        
        
        
//Elev, Kurs, kursbetyg
String st = "";
        
        try{
            st = Getter.getCourseGrade(hwdb, jTextField1.getText(), jTextField2.getText(), tf_startdatum.getText());
            jTextArea1.setText(st);
        }
        catch(Exception e){
            
        }
        
        
        
        
//Prefekt för elevhem
String dormitory = jComboBox1.getSelectedItem().toString();
        String st = "";
        String yes = "Prefekt för";
        if(dormitory.isEmpty()){
            st ="";
            jTextArea1.setText("Välj elevhem");
        }
        else
        
        st = Getter.getPrefektDormitory(hwdb, dormitory);
        jTextArea1.setText(yes + " " + dormitory + "\n" +st);
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
